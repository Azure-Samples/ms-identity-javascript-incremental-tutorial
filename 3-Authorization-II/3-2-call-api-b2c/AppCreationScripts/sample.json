{
    "Sample": {
      "Title": "Vanilla JavaScript Single-page Application (SPA) using MSAL.js to authorize users for calling a protected web API on Azure AD B2C",
      "Level": 200,
      "Client": "Vanilla JavaScript SPA",
      "Service": "Node web API",
      "RepositoryUrl": "ms-identity-b2c-javascript-ch3-s2",
      "Endpoint": "AAD v2.0",
      "Provider": "B2C"
    },

    "AADApps": [
      {
        "Id": "spa",
        "Name": "ms-identity-javascript-tutorial-ch3-s2-client",
        "Kind" :  "SinglePageApplication",
        "Audience": "AzureADandPersonalMicrosoftAccount",
        "HomePage": "http://localhost:6420",
        "ReplyUrls":"http://localhost:6420",
        "RequiredResourcesAccess": [
            {
              "Resource": "service",
              "DelegatedPermissions": [ "demo.read" ]
            }
          ],
        "ManualSteps": [
          {
            "Comment": "Navigate to the Manifest page, find the 'replyUrlsWithType' section and change the type of redirect URI to 'Spa'"
          }
        ]
      },
      {
        "Id": "service",
        "Name": "ms-identity-javascript-tutorial-ch3-s1-service",
        "Kind": "WebApi",
        "HomePage": "http://localhost:5000/api",
        "Audience": "AzureADAnyOrgandPersonalMicrosoftAccount",
        "ManualSteps": [
          {
            "Comment": "Navigate to the Manifest page, find the property 'accessTokenAcceptedVersion' and set it to '2'"
          }
        ]
      }
    ],
  
    "CodeConfiguration": [
      {
        "App": "spa",
        "SettingKind": "Replace",
        "SettingFile": "\\..\\App\\authConfig.js",
        "Mappings": [
          {
            "key": "Enter_the_Application_Id_Here",
            "value": ".AppId"
          },
          {
            "key": "Enter_the_Cloud_Instance_Id_Here/Enter_the_Tenant_Info_Here",
            "value": "\"https://login.microsoftonline.com/\"+$tenantName"
          },
          {
            "key": "Enter_the_Redirect_Uri_Here",
            "value": ".HomePage"
          }
        ]
      },
      {
        "App": "spa",
        "SettingKind": "Replace",
        "SettingFile": "\\..\\App\\authConfig.js",
        "Mappings": [
          {
            "key": "Enter_the_Web_Api_Uri_Here",
            "value": "service.HomePage"
          },
          {
            "key": "Enter_the_Web_Api_Scope_Here",
            "value": "service.Scope"
          }
        ]
      },
      {
        "App": "service",
        "SettingKind": "JSON",
        "SettingFile": "\\..\\config.json",
        "Mappings": [
          {
            "key": "clientID",
            "value": ".AppId"
          },
          {
            "key": "tenantName",
            "value": "$tenanName"
          },
          {
            "key": "audience",
            "value": ".AppId"
          }
        ]
      }
    ]
  }