{
    "Sample": {
      "Title": "Vanilla JavaScript Single-page Application secured with MSAL.js 2.x using Authorization Code Flow (w/ PKCE) to call a custom web API protected by Azure Active Directory",
      "Level": 200,
      "Client": "Vanilla JavaScript SPA",
      "Service": "Microsoft Graph",
      "RepositoryUrl": "ms-identity-javascript-v2",
      "Endpoint": "AAD v2.0"
    },

    "AADApps": [
      {
        "Id": "spa",
        "Name": "ms-identity-javascript-v2",
        "Kind" :  "SinglePageApplication",
        "HomePage": "http://localhost:3000/",
        "ReplyUrls":"http://localhost:3000/",
        "RequiredResourcesAccess": [
          {
            "Resource": "Microsoft Graph",
            "DelegatedPermissions": [ "User.Read" ]
          }
        ],
        "ManualSteps": [
            {
              "Comment": "Navigate to the Manifest page, find the 'replyUrlsWithType' section and change the type of redirect URI to 'Spa'"
            },
            { 
              "Comment": "This sample needs to be coupled with a Web API and as such, you'll need to manually add the necessary API permissions and update the configuration files accordingly."
            }
        ]
      }
    ],
  
    "CodeConfiguration": [
      {
        "App": "spa",
        "SettingKind": "Replace",
        "SettingFile": "\\..\\app\\authConfig.js",
        "Mappings": [
          {
            "key": "Enter_the_Application_Id_Here",
            "value": ".AppId"
          },
          {
            "key": "Enter_the_Cloud_Instance_Id_Here/Enter_the_Tenant_Info_Here",
            "value": "\"https://login.microsoftonline.com/\"+$tenantId"
          },
          {
            "key": "Enter_the_Redirect_Uri_Here",
            "value": ".ReplyUrls"
          }
        ]
      }
    ]
  }